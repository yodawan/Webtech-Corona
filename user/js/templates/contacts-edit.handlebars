		    <div class="form-group">
		      <label for="editname" class="col-lg-3 control-label">Package Name</label>
		      <div class="col-lg-9">
		        <input type="text" class="form-control" id="editname" value="{{name}}" required>
		      </div>
		    </div>
		    <div class="form-group">
		      <label for="editemail" class="col-lg-3 control-label">Price</label>
		      <div class="col-lg-9">
		        <input type="text" class="form-control" id="editemail" value="{{email}}" required>
		      </div>
		    </div>
		    <div class="form-group">
		      <label for="editmobileno" class="col-lg-3 control-label">Total Day</label>
